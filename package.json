{
  "name": "visa",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "codegen": "graphql-codegen --config codegen.ts",
    "migration/create": "bunx tsx ./scripts/createMigration.ts",
    "migration/apply": "bun supabase migration up",
    "status": "bun supabase status",
    "db/seed/dry": "bunx tsx seed.mts",
    "db/seed": "DRY=0 bunx tsx seed.mts",
    "db/reset": "bun supabase db reset && bun db:seed",
    "db/pull": "dotenv -- cross-var bun supabase db pull -p %SUPABASE_DB_PASSWORD%",
    "db/push": "dotenv -- cross-var bun supabase db push -p %SUPABASE_DB_PASSWORD%",
    "types/generate/server": "bunx snaplet generate && bunx tsx ./scripts/createTsFromDB.ts",
    "types/generate/local": "bunx snaplet generate && bunx tsx ./scripts/createTsFromDB.ts local",
    "graphql/generate": "graphql-codegen"
  },
  "dependencies": {
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@tanstack/query-core": "^5.28.9",
    "assemblyai": "^4.3.3",
    "isbot": "^5.1.3",
    "puppeteer": "^22.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supabase": "^1.151.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "5.11.0",
    "@remix-run/dev": "^2.8.1",
    "@snaplet/copycat": "5.0.0",
    "@snaplet/seed": "0.89.6",
    "@supabase/supabase-js": "^2.41.1",
    "@types/dom-speech-recognition": "^0.0.4",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "cross-var": "^1.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prisma": "^5.11.0",
    "snaplet": "0.89.6",
    "typescript": "5.4.3",
    "vite": "^5.2.7",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "schema": "supabase/schema.prisma"
  }
}