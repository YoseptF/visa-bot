{
  "name": "visa",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "codegen": "graphql-codegen --config codegen.ts",
    "migration/create": "bunx tsx ./scripts/createMigration.ts",
    "migration/apply": "bun supabase migration up",
    "status": "bun supabase status",
    "db/seed/dry": "bunx tsx seed.mts",
    "db/seed": "DRY=0 bunx tsx seed.mts",
    "db/reset": "bun supabase db reset && bun db:seed",
    "db/pull": "dotenv -- cross-var bun supabase db pull -p %SUPABASE_DB_PASSWORD%",
    "db/push": "dotenv -- cross-var bun supabase db push -p %SUPABASE_DB_PASSWORD%",
    "types/generate/server": "bunx snaplet generate && bunx tsx ./scripts/createTsFromDB.ts",
    "types/generate/local": "bunx snaplet generate && bunx tsx ./scripts/createTsFromDB.ts local",
    "graphql/generate": "graphql-codegen"
  },
  "dependencies": {
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supabase": "^1.127.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "5.11.0",
    "@remix-run/dev": "^2.8.1",
    "@snaplet/copycat": "2.0.0",
    "@snaplet/seed": "0.86.3",
    "@supabase/supabase-js": "^2.39.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "cross-var": "^1.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prisma": "^5.7.1",
    "snaplet": "0.86.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "schema": "supabase/schema.prisma"
  }
}